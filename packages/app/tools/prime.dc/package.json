{
  "name": "firebase-prime",
  "description": "[CLI] Priming an emulated Firestore instance with users and data, from ESM files.",
  "license": "MIT",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "files": [
    "src/**"
  ],
  "bin": {
    "firebase-prime": "src/index.js"
  },
  "dependencies": {
    "@_/self": "file:.",
    "commander": "^9.1.0",
    "firebase-admin": "^10.2.0"
  },
  "//": {
    "usage": [
      "FIREBASE_JSON=<path> firebase-prime [--host=abc] --project=xyz docs.js users.js"
    ],
    "self-dependency": [
      "Needed for 'npx firebase-prime' to work."
    ],
    "engines.node": [
      ">=18: provides 'fetch' API natively"
    ]
  }
}
